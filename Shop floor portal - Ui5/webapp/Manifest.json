{
	"_version": "1.7.0",
	"sap.app": {
		"id": "SHOP_FLOOR",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZPP_SHOPFLOOR_RT_SRV": {
				"uri": "/sap/opu/odata/sap/ZPP_SHOPFLOOR_RT_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "SHOP_FLOOR.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "SHOP_FLOOR.view",
				"targetAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "Login",
					"view": "Login",
					"targetAggregation": "pages",
					"controlId": "app"
				},
				{
					"pattern": "Dashboard",
					"name": "Dashboard",
					"view": "Dashboard",
					"targetAggregation": "pages",
					"controlId": "app"
				},
				{
					"pattern": "PlanningOrder",
					"name": "PlanningOrder",
					"view": "PlanningOrder",
					"targetAggregation": "pages",
					"controlId": "app"
				},
				{
					"pattern": "ProductionOrder",
					"name": "ProductionOrder",
					"view": "ProductionOrder",
					"targetAggregation": "pages",
					"controlId": "app"
				}
			]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "SHOP_FLOOR.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}